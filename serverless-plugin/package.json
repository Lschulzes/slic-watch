{
  "name": "serverless-slic-watch-plugin",
  "version": "3.0.0-rc1",
  "description": "Serverless plugin for automatic alarms and dashboards",
  "main": "dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fourTheorem/slic-watch.git",
    "directory": "serverless-plugin"
  },
  "scripts": {
    "test": "tap --coverage-report=html --no-browser --no-check-coverage tests/**/*.test.ts",
    "prepublish": "cp ../README.md README.md",
    "build": "esbuild ./index.ts --outfile=dist/index.js --platform=node --format=esm --packages=external --bundle --sourcemap",
    "watch": "npm run build -- --watch"
  },
  "devDependencies": {
    "serverless": "^3.28.1",
    "serverless-hooks-plugin": "^1.1.0"
  },
  "engines": {
    "node": ">=10.0"
  },
  "license": "Apache",
  "dependencies": {
    "ajv": "^8.11.0",
    "slic-watch-core": "^2.0.0-rc4"
  },
  "gitHead": "c4300240f9e854c4eb1c5503b839882ff0cd1cae"
}
